{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.1",
    "permissions": ["activeTab", "storage"],
    "icons": {
        "16": "icons/icon.png",
        "32": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      },
    "action": {
        "default_icon": {              
            "16": "icons/icon.png",   
            "24": "icons/icon.png",   
            "32": "icons/icon.png"
        },
        "default_title": "ShortCut",
        "default_popup": "index.html"
    }
}